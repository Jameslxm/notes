<?xml version="1.0" encoding="utf-8"?><LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"              android:layout_width="match_parent"              android:layout_height="match_parent"              android:orientation="vertical">    <TextView        android:id="@+id/tvContent"        android:layout_width="wrap_content"        android:layout_height="wrap_content"/>    <Button        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:onClick="writeData"        android:text="加密方式写入数据"/>    <Button        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:onClick="readData"        android:text="读取加密数据"/>    <Button        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:onClick="readOriginData"        android:text="读取原始数据"/>    <Button        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:onClick="spWrite"        android:text="sp加密"/>    <Button        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:onClick="spRead"        android:text="sp解密"/>    <Button        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:onClick="spReadEncryption"        android:text="sp加密之后的数据"/></LinearLayout>