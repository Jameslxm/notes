intTop_toBottomOf="@+id/btn_factory"            android:text="外观模式"/>        <Button            android:id="@+id/btn_event_conflict"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            app:layout_constraintLeft_toLeftOf="parent"            app:layout_constraintTop_toBottomOf="@+id/btn_appearance"            android:text="事件冲突"/>        <Button            android:id="@+id/btn_px_adapter"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            app:layout_constraintLeft_toLeftOf="parent"            app:layout_constraintTop_toBottomOf="@+id/btn_event_conflict"            android:text="自定义像素适配"/>        <Button            android:id="@+id/btn_density"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            app:layout_constraintLeft_toLeftOf="parent"            app:layout_constraintTop_toBottomOf="@+id/btn_px_adapter"            android:text="密度适配"/>        <!--    <Button-->        <!--        android:id="@+id/btn_go_login"-->        <!--        android:layout_width="wrap_content"-->        <!--        android:layout_height="wrap_content"-->        <!--        app:layout_constraintStart_toStartOf="parent"-->        <!--        app:layout_constraintTop_toTopOf="parent"-->        <!--        android:text="去登陆页"/>-->        <!--    <TextView-->        <!--        android:id="@+id/tv_content"-->        <!--        android:layout_width="wrap_content"-->        <!--        android:layout_height="wrap_content"-->        <!--        app:layout_constraintStart_toStartOf="parent"-->        <!--        app:layout_constraintTop_toTopOf="parent"-->        <!--        app:layout_constraintBottom_toBottomOf="parent"-->        <!--        />-->        <!--    <EditText-->        <!--        android:id="@+id/et_input"-->        <!--        android:layout_width="match_parent"-->        <!--      